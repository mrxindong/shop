    <add key="ConnectionString" value="server=127.0.0.1;database=codematic;uid=sa;pwd="/><!--8618B53C32BF8E0B6CD3BDFF59B9F24BE1C6D586A5915C8B6DC69A09ABB2DF60A4026BDE8E76B6FDE2BFCEA3FA8E06CB-->
